[core]
  editor = vim
	excludesfile = ~/.gitignore

[user]
	name = Brice Stacey
	email = bricestacey@gmail.com

[color]
  ui = true

[merge]
  conflictstyle = diff3

[alias]
   fap = fetch --all --prune
   fop = fetch origin --prune
   l = log --graph --decorate --pretty=oneline --abbrev-commit
   la = log --graph --decorate --pretty=oneline --abbrev-commit --all
